{
  "name": "lint-scroll-marker",
  "main": "./lib/lint-scroll-marker",
  "version": "0.3.0",
  "description": "Highlights linter errors on the scroll bar",
  "keywords": [
    "scroll bar",
    "scrollbar",
    "highlight",
    "linter",
    "nuclide",
    "atom-ide-ui"
  ],
  "repository": "https://github.com/surdu/lint-scroll-marker",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "consumedServices": {
    "scroll-marker": {
      "versions": {
        "0.1.0": "consumeScrollMarker"
      }
    },
    "diagnostics-store": {
      "versions": {
        "0.3.0": "consumeDiagnosticUpdates"
      }
    }
  },
  "providedServices": {
    "linter-ui": {
      "versions": {
        "1.0.0": "provideUI"
      }
    }
  },
  "dependencies": {
    "atom-package-deps": "^4.6.2"
  },
  "package-deps": [
    "scroll-marker"
  ]
}
